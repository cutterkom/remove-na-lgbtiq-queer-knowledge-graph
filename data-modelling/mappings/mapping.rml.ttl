@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix ex: <http://example.com/>.
@prefix schema: <https://schema.org/>.
@prefix wd: <http://www.wikidata.org/entity/>.

:rules_000 a void:Dataset;
    void:exampleResource :map_BookMapping_000.
:map_BookMapping_000 rml:logicalSource :source_000.
:source_000 a rml:LogicalSource;
    rml:source "data.csv";
    rml:referenceFormulation ql:CSV.
:map_BookMapping_000 a rr:TriplesMap;
    rdfs:label "BookMapping".
:s_000 a rr:SubjectMap.
:map_BookMapping_000 rr:subjectMap :s_000.
:s_000 rr:template "http://example.com/{book_id}".
:pom_000 a rr:PredicateObjectMap.
:map_BookMapping_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant rdf:type.
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rr:constant "https://schema.org/Book";
    rr:termType rr:IRI.
:pom_001 a rr:PredicateObjectMap.
:map_BookMapping_000 rr:predicateObjectMap :pom_001.
:pm_001 a rr:PredicateMap.
:pom_001 rr:predicateMap :pm_001.
:pm_001 rr:constant schema:name.
:pom_001 rr:objectMap :om_001.
:om_001 a rr:ObjectMap;
    rml:reference "name";
    rr:termType rr:Literal.
:pom_002 a rr:PredicateObjectMap.
:map_BookMapping_000 rr:predicateObjectMap :pom_002.
:pm_002 a rr:PredicateMap.
:pom_002 rr:predicateMap :pm_002.
:pm_002 rr:constant schema:isbn.
:pom_002 rr:objectMap :om_002.
:om_002 a rr:ObjectMap;
    rml:reference "isbn";
    rr:termType rr:Literal.
